{% extends 'base.html.twig' %}

{% block body %}

    <div class="wrapper">
        {% include 'parts/header.html.twig' %}
        <div class="wrap_12">

            <div class="wrap_content">
                {% include 'parts/menu.html.twig' %}
                <div class="main">
                    <div class="container">

                        <div class="content_head">

                            <div class="h2">Инициация</div>

                        </div>

                        <div class="content_item content_item9">

                            <div class="scroll-pane">

                                <div class="form_ans">

                                    <div class="wrap_anket clearfix">

                                        <form class="form">

                                            <input class="input" type="text" maxlength="" name="ans1" placeholder="Вопрос...">

                                            <input class="input" type="text" maxlength="" name="ans2" placeholder="Вопрос...">

                                            <div class="fileload">
                                                <div class="file-load-block">
                                                    <input type="file" id="file" class="inputFile" accept="image/*"  onchange='document.getElementById("fileName").value=this.value'/>
                                                    <div class="fileLoad">
                                                        <button class="load btn-submit">Загрузить фото <sup>*</sup></button>
                                                    </div>
                                                    <input type="text" class="inputFileVal" readonly="readonly" id="fileName" value="" />
                                                </div>
                                            </div>


                                            <input class="btn-submit" type="submit" value="Отправить">

                                        </form>

                                    </div>

                                    <div class="bottom">
                                        <div class="bottom_text">
                                            {#<sup>*</sup> сноска....#}
                                        </div>
                                    </div>

                                </div>
                            </div>



                        </div>

                    </div>
                </div>
            </div>
        </div> <!-- wrap_12-->
    </div>
{% endblock %}

{% block stylesheets %}
{% endblock %}
